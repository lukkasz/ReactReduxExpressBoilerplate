{
  "name": "react-redux-express-boilerplate",
  "version": "1.0.0",
  "description": "React, Redux, Express boilerplate with Babel support",
  "main": "app.jsx",
  "repository": "git@github.com:lukkasz/ReactReduxExpressBoilerplate.git",
  "scripts": {
    "test": "NODE_PATH=./ mocha --compilers js:babel-core/register --require ./app/tests/setup.js  'app/tests/**/*.test.jsx'",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base ./public",
    "start": "npm run build && node ./server",
    "build": "node ./node_modules/webpack/bin/webpack.js -p --config ./webpack.production.config.js"
  },
  "author": "lukkasz",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "bootstrap": "^3.3.6",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.6.0",
    "file-loader": "^0.9.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "script-loader": "^0.6.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "express": "^4.13.4",
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "redux": "^3.3.1"
  }
}
